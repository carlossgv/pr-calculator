{
  "name": "pr-calculator-web",
  "private": true,
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "prepare": "husky",
    "dev": "turbo dev",
    "build": "pnpm -r --if-present build",
    "typecheck": "turbo typecheck",
    "test": "pnpm -r --if-present test",
    "prisma:generate": "prisma generate --config ./prisma.config.ts",
    "prisma:migrate": "prisma migrate dev --config ./prisma.config.ts",
    "prisma:push": "prisma db push --config ./prisma.config.ts",
    "prisma:studio": "prisma studio --config ./prisma.config.ts",
    "prisma:reset": "prisma migrate reset --force --config ./prisma.config.ts && prisma generate --config ./prisma.config.ts",
    "prisma:seed": "prisma db seed --config ./prisma.config.ts"
  },
  "devDependencies": {
    "husky": "^9.1.7",
    "turbo": "^2.8.7",
    "typescript": "^5.6.3"
  }
}
